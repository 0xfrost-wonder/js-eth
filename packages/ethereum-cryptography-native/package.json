{
  "name": "ethereum-cryptography-native",
  "version": "0.0.2",
  "description": "Native implementations of ethereum-cryptography for Node.js",
  "author": "Patricio Palladino <patricio@nomiclabs.io>",
  "repository": "github:alcuadrado/ethereum-cryptography",
  "license": "MIT",
  "keywords": [
    "ethereum",
    "cryptography"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "src",
    "*.js",
    "*.js.map",
    "*.d.ts",
    "*.d.ts.map"
  ],
  "sideEffects": false,
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.prod.json",
    "test": "mocha",
    "clean": "rimraf '*.js' '*.js.map' '*.d.ts' '*.d.ts.map'",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --fix --project tsconfig.json"
  },
  "devDependencies": {
    "@types/chai": "^4.2.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^8.10.52",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "prettier": "1.18.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.6.2"
  },
  "optionalDependencies": {
    "scrypt": "^6.0.3",
    "sha3": "^1.2.3"
  }
}
